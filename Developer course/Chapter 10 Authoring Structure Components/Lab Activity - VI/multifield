use( function () {

    var menuList = [];
    var menuArray = properties.get("tabMenu");
    if(menuArray != null){

        for(var i = 0; i < menuArray.length; i++) {
        var singleObj = {};
        var itemObject =  JSON.parse(menuArray[i]);
        singleObj['title'] = itemObject.title;
        singleObj['path'] = itemObject.path;
        singleObj['icon'] = itemObject.icon;
        singleObj['icon2'] = itemObject.icon2;
        singleObj['alttext'] = itemObject.alttext;
        singleObj['titletext'] = itemObject.titletext;
        menuList.push(singleObj);
    };
    }

   return {
        menuList: menuList
    };
});
